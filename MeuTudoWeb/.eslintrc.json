{
	"env": {
		"amd": true,
		"jquery": true,
		"es6": true
	},
	"globals": {
		"console": true,
		"document": true,
		"window": true,
		"describe": true,
		"module": true,
		"process": true,
		"it": true,
		"expect": true,
		"navigator": true,
		"__dirname": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"parser": "babel-eslint",
	"rules": {
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-unused-vars": "off",
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"space-before-blocks": "error",
		"space-infix-ops": "error",
		"comma-spacing": "error",
		"key-spacing": "error",
		"object-curly-spacing": [
			"error",
			"always"
		],
		"space-before-function-paren": [
			"error",
			{
				"anonymous": "always",
				"named": "never"
			}
		],
		"keyword-spacing": "error",
		"no-mixed-spaces-and-tabs": "error",
		"semi": "off",
		"brace-style": [
			"error",
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"func-style": [
			"error",
			"declaration",
			{
				"allowArrowFunctions": true
			}
		],
		"no-sparse-arrays": "error",
		"eqeqeq": [
			"warn",
			"smart"
		],
		"no-else-return": "warn",
		"indent": [
			"error",
			2,
			{
				"SwitchCase": 1
			}
		]
	}
}